{
  "name": "myapp",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "sync": "npm run sync:pmtiles && npm run sync:parquet",
    "sync:pmtiles": "rclone sync --progress --s3-no-check-bucket --s3-chunk-size=256M --include=*.pmtiles static/pmtiles r2://hdx-boundaries-explorer/pmtiles",
    "sync:parquet": "rclone sync --progress --s3-no-check-bucket --s3-chunk-size=256M --include=*.parquet static/parquet r2://hdx-boundaries-explorer/parquet",
    "pmtiles": "./scripts/pmtiles.sh",
    "parquet": "./scripts/parquet.sh",
    "update": "npx npm-check-updates -u && npm install"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^10.0.1",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.52.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/node": "^25",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.15.0",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte": "^5.51.2",
    "svelte-check": "^4.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "hyparquet": "^1.25.0",
    "hyparquet-compressors": "^1.1.1",
    "maplibre-gl": "^5.18.0",
    "pmtiles": "^4.4.0"
  }
}
